{
  "manifest_version": 3,
  "name": "Save Page as Markdown",
  "version": "1.0",
  "permissions": ["scripting", "activeTab", "downloads"],
  "action": {
    "default_title": "Save as Markdown"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "host_permissions": ["<all_urls>"]
}
